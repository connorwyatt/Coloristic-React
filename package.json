{
  "name": "coloristic-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-observable": "^0.17.0",
    "rxjs": "^5.5.5"
  },
  "devDependencies": {
    "flow-bin": "^0.60.1",
    "flow-typed": "^2.2.3",
    "husky": "^0.14.3",
    "prettier": "^1.9.1",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "postinstall": "flow-typed install",
    "precommit": "yarn run check-format && yarn run flow",
    "flow": "flow",
    "format": "prettier --write \"**/*.js\" \"**/*.json\"",
    "check-format":
      "prettier --list-different \"**/*.js\" \"**/*.json\""
  }
}
